{
  "name": "stencil",
  "version": "0.0.1",
  "scripts": {
    "build": "../../node_modules/.bin/tsc -p tsconfig.schematics.json",
    "copy:schemas": "rsync -R schematics/*/schema.json ../../dist/stencil/",
    "copy:files": "rsync -R -p schematics/*/files/** ../../dist/stencil/",
    "copy:collection": "cp -f schematics/collection.json ../../dist/stencil/schematics/collection.json",
    "postbuild": "yarn copy:schemas && yarn copy:files && yarn copy:collection"
  },
  "peerDependencies": {
    "@angular/common": "^9.0.0-rc.3",
    "@angular/core": "^9.0.0-rc.3",
    "tslib": "^1.10.0"
  },
  "schematics": "./schematics/collection.json"
}
