{
  "name": "ng-basis",
  "version": "0.2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --code-coverage --watch false --progress false",
    "test:ci": "yarn test --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:ci": "ng e2e --protractor-config=e2e/protractor-ci.conf.js",
    "build:stencil": "yarn build stencil && yarn tsc -p './projects/stencil/tsconfig.schematics.json'",
    "postbuild:stencil": "yarn copy:schemas && yarn copy:files && yarn copy:collection",
    "copy:schemas": "cd projects/stencil && rsync -R schematics/*/schema.json ../../dist/stencil/",
    "copy:files": "cd projects/stencil && rsync -R -p schematics/*/files/** ../../dist/stencil/",
    "copy:collection": "cp -f projects/stencil/schematics/collection.json dist/stencil/schematics/collection.json",
    "test:stencil": "jasmine './dist/stencil/schematics/**/*.spec.js'",
    "stylelint": "stylelint './**/*.scss'",
    "prettier": "prettier --write './**/*.{ts,html,scss}'",
    "prettier:check": "prettier --check './**/*.{ts,html,scss}'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.0-rc.3",
    "@angular/common": "9.0.0-rc.3",
    "@angular/compiler": "9.0.0-rc.3",
    "@angular/core": "9.0.0-rc.3",
    "@angular/forms": "9.0.0-rc.3",
    "@angular/platform-browser": "9.0.0-rc.3",
    "@angular/platform-browser-dynamic": "9.0.0-rc.3",
    "@angular/router": "9.0.0-rc.3",
    "angular-cli-ghpages": "0.6.0",
    "rxjs": "6.5.3",
    "tslib": "1.10.0",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.900.0-rc.3",
    "@angular-devkit/build-ng-packagr": "~0.900.0-rc.3",
    "@angular/cli": "9.0.0-rc.3",
    "@angular/compiler-cli": "9.0.0-rc.3",
    "@angular/language-service": "9.0.0-rc.3",
    "@types/jasmine": "3.4.0",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "12.11.1",
    "codelyzer": "5.1.2",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.3.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "ng-packagr": "^9.0.0-rc.1",
    "prettier": "1.19.1",
    "protractor": "5.4.2",
    "stylelint": "12.0.0",
    "stylelint-config-standard": "19.0.0",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "typescript": "3.6.4"
  }
}
